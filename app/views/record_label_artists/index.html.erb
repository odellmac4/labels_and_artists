<% @artists.each do |artist|%>
    <h1><%= artist.name %></h1>
    <p>Grammy Award Winning: <%= artist.grammy_winning %>
    <p>Total Streams: <%= artist.total_streams %>
    <p>Highest Charting Song: <%= artist.highest_charted_song %>
    <br>
<% end %>
<%= link_to 'Add New Artist', "/recordlabels/#{@record_label.id}/artists/new" %>
